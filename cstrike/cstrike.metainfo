# App 名称
APP_NAME="cstrike"
# App 中文名称,首次启动时需要显示
APP_NAME_CN="cstrike"
# App 版本号
APP_VERSION="1.6"
# App 压缩包文件名
APP_ARCHIVE="cstrike.tar.zst"
# App 版本升级时需要忽略的文件列表
APP_IGNORE=".appignore"
# App 运行时需要进入的路径
APP_RUN_PATH="drive_c/Games/CS 1.6 Original"
# App 运行命令
APP_RUN_CMD="hl.exe -steam -game cstrike -appid 10 -noipx -nojoy -noforcemspd -noforcemparms -noforcemaccel"
# App 运行时使用的 wine 版本号，当需要使用与默认wine不同的版本时需要指定。
APP_RUN_WINE="6.0"
