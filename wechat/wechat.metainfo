# App 名称
APP_NAME="wechat"
# App 中文名称,首次启动时需要显示
APP_NAME_CN="微信"
# App 版本号
APP_VERSION="1.0"
# App 压缩包文件名
APP_ARCHIVE="wechat.tar.xz"
# App 版本升级时需要忽略的文件列表
APP_IGNORE=".appignore"
# App 运行时需要进入的路径
APP_RUN_PATH="drive_c/Program Files/Tencent/WeChat"
# App 运行命令
APP_RUN_CMD="WeChat.exe"
# App 运行时使用的 wine 版本号，当需要使用与默认wine不同的版本时需要指定。
APP_RUN_WINE="7.20"
