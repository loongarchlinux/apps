pkgname=linuxqq
pkgver=3.2.6
pkgbld=22165
pkgrel=1
pkgdesc="New Linux QQ based on Electron"
arch=(loong64)
url="https://im.qq.com/linuxqq/index.shtml"
license=(custom)
groups=('apps')
depends=(liblol)
options=(strip !debug)
source=("https://dldir1.qq.com/qqfile/qq/QQNT/e619e933/${pkgname}_${pkgver}-${pkgbld}_loongarch64.deb"
    linuxqq.sh)
sha256sums=('cba933f374c97f4fec767ce5afbefe7f937f1b2ed910b3cc2b9c6df25218cccf'
            'fcc8ab360be9fde853b9a4279251072df6807a3bd697677cd8175fb0b89d9be7')

package() {
  tar -C ${pkgdir} -xf data.tar.xz
  install -Dm755 ${srcdir}/linuxqq.sh ${pkgdir}/usr/bin/linuxqq
}
